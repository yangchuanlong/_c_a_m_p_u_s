.container{
    width: 100vw;
    height: 100vh;
    position: relative;
    background-color: #fbfbfc;
    display:block;
    padding: 0;
}
.bgImg{
    height: 516rpx;
    width: 100vw;
    position: absolute;
    left: 0;
    top: 0;
}
.header{
    width: 100%;
    z-index: 2;
    position: relative;
    height: 445rpx;
    padding-top: 1rpx;
}
.userCollegeGrade{
    display:flex;
    justify-content:center;
    font-size: 26rpx;
    letter-spacing: 1rpx;
    margin-top: 20rpx;
    color: #fff;
}
.grade{
    margin-left: 16rpx;
}

.nickName{
    text-align: center;
    font-size: 32rpx;
    color: #fff;
    margin-top: 32rpx;
}
.avatar {
    margin: 100rpx auto 0;
    width: 140rpx;
    height: 140rpx;
    display: block;
    border-radius: 50%;
}
.main{
    z-index:3;
    position:relative;
    box-sizing: border-box;
    width: 690rpx;
    margin: 0 auto;
    overflow: hidden;
}
.main navigator{    
    display: flex;
    font-size: 30rpx;
    color: #6e7b8c;
    align-items: center;
    height: 140rpx;
    background: white;
    box-sizing: border-box;
    padding: 0 55rpx;
    border-radius: 10rpx;
    box-shadow: 0px 5px 50px 0px #eef1f5;
}
.main navigator image{
    width: 80rpx;
    height: 80rpx;
    margin-right: 30rpx;
}
.main navigator:not(:first-child) {
  margin-top: 40rpx;
}

.main navigator .rightArrow{
  color: #b5bdc5;
}
.main .navigator-hover:first-child{
    opacity: 1;
}